<script>
    $(document).ready(function () {
        $("#loginBtn").on("click", function () {
            $("#loginForm").submit();
        });
        $("body").keydown(function (e) {
            if (e.keyCode == "13") {//keyCode=13是回车键
                $("#loginForm").submit();
            }
        });
        $("#loginForm").validate();
    });
</script>

